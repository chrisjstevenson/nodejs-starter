{
"collection": {
"version": "1.0",
"href": "<%= site %>",
"items": [
    <%for(i=0,x=items.length;i<x;i++) { %>
        {
            "href": "<%=site%><%=items[i]._id%>",
            "data": [
                {
                    "name": "description",
                    "value":  "<%=items[i].description%>",
                    "prompt": "Description"
                },
                {
                    "name": "completed",
                    "value": "<%=items[i].completed%>",
                    "prompt": "Completed"
                },
                {
                    "name": "dateDue",
                    "value": "<%=items[i].dateDue%>",
                    "prompt": "Date Due"
                }
            ]
        }
    <% } %>
],
"queries": [],
"template": {
"data": []
},
"error": {}
}
}